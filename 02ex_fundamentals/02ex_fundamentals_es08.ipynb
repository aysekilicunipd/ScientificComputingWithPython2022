    "def recursiveFibonacci(n):  \n",
    "    if n <= 1:  \n",
    "        return n  \n",
    "    else:  \n",
    "        return(recursiveFibonacci(n-1) + recursiveFibonacci(n-2))\n",
    "    \n",
    "length = 20  \n",
    "for i in range(length):\n",
    "    print(recursiveFibonacci(i)) "
