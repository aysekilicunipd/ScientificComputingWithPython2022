    "def square(num):\n",
    "    return num*num\n",
    "\n",
    "def cube(num):\n",
    "    return num*num*num\n",
    "    \n",
    "print(square(2))\n",
    "print(cube(2))\n",
    "\n",
    "def sixth_pow(num):\n",
    "    return cube(square(num))\n",
    "\n",
    "print(sixth_pow(2))"
